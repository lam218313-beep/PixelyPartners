# Pixely Partners Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
# OpenAI model to use for analyses (e.g., gpt-5-nano)
OPENAI_MODEL=gpt-5-nano

# Orchestrator Configuration
ORCHESTRATOR_USER=orchestrator
ORCHESTRATOR_PASSWORD=secure_password_here

# Outputs Directory (optional, defaults to orchestrator/outputs)
PIXELY_OUTPUTS_DIR=/app/orchestrator/outputs
